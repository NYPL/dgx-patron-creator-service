{
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["jest"],
  "rules": {
    "import/extensions": 0,
    "global-require": 0,
    "prefer-destructuring": 0,
    "class-methods-use-this": 0,
    "function-paren-newline": 0,
    "quotes": [2, "double"]
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  }
}
