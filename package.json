    "aws-sdk": "^2.49.0",
    "aws-serverless-express": "^2.1.3",
    "axios": "0.15.3",
    "body-parser": "1.15.2",
    "dotenv": "^4.0.0",
    "eslint": "0.24.0",
    "express": "4.12.3",
    "pg-promise": "5.3.5",
    "promise": "7.1.1",
