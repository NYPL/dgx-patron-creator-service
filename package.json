{
  "name": "dgx-patron-creator-service",
  "version": "0.1.1",
  "description": "NYPL Patron Creator Service",
  "main": "app.js",
  "scripts": {
    "start": "swagger project start",
    "swagger-edit": "swagger project edit",
    "build-swagger-doc": "yaml2json ./api/swagger/swagger.yaml > ./api/swagger/swaggerDoc.json",
    "deploy-development": "./node_modules/.bin/node-lambda deploy -e development -f ./config/deploy_development.env -S ./config/event_sources_development.json -o arn:aws:iam::224280085904:role/lambda_basic_execution -a $AWS_ACCESS_KEY_ID_DEVELOPMENT -s $AWS_SECRET_ACCESS_KEY_DEVELOPMENT",
    "deploy-development-from-local": "./node_modules/.bin/node-lambda deploy -e development -f ./config/deploy_development.env -S ./config/event_sources_development.json -o arn:aws:iam::224280085904:role/lambda_basic_execution -p nypl-sandbox",
    "deploy-qa": "TBD",
    "deploy-production": "TBD",
    "build-doc-deploy-package-qa": "npm run build-swagger-doc && npm run deploy-package-qa",
    "build-doc-deploy-package-production": "npm run build-swagger-doc && npm run deploy-package-production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "expess",
    "postgresql",
    "swagger"
  ],
  "author": "NYPL Digital Experience Team",
  "license": "",
  "bugs": {
    "url": "https://bitbucket.org/NYPL/patron-creator-service"
  },
  "dependencies": {
    "avsc": "5.0.1",
    "aws-sdk": "2.49.0",
    "aws-serverless-express": "2.1.3",
    "axios": "0.15.3",
    "body-parser": "1.15.2",
    "dotenv": "4.0.0",
    "express": "4.12.3",
    "jest": "^23.2.0",
    "pg-promise": "5.3.5",
    "promise": "7.1.1",
    "serverless": "1.9.0",
    "serverless-plugin-write-env-vars": "1.0.1",
    "swagger-express-mw": "0.6.0",
    "swagger-tools": "0.10.1"
  },
  "devDependencies": {
    "assert": "1.4.1",
    "aws-sdk-mock": "4.0.0",
    "chai": "4.1.2",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.7.0",
    "jest": "23.1.0",
    "should": "7.1.0",
    "sinon": "6.0.1",
    "supertest": "1.0.0"
  }
}
